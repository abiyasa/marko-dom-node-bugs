<site-layout>
  <@title>
    ui-textual-display
  </@title>

  <@content>
    <section>
      <h2>UI Textual Display</h2>
      <p>A demo rendering textual display with fixtures (see fixtures folder for the content). No bugs!</p>

      <for(fixture in input.params.fixtures)>
        <h3>${fixture.name}</h3>

        $ data = fixture.data || {};
        <ui-textual-display ...data />
      </for>
    </section>
  </@content>
</site-layout>
